using System;

namespace DeclaringVariablesPt3
{
    class MainClass
    {
        public static void Main(string[] args)
        {




            int x = 7;
            string y = "Bob";
            string myFirstTry = x.ToString() + y;

            //int mySecondTry = x + y;
            intMySecondTry = x + int.Parse(y); 

            Console.Write(myFirstTry);


            Console.ReadLine();
        }
    }
}
